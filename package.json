{
  "name": "docker-context-status",
  "displayName": "Docker Context Status",
  "description": "Shows current Docker context in the VS Code status bar",
  "version": "1.0.1",
  "publisher": "valix",
  "repository": {
    "type": "git",
    "url": "https://github.com/valix/vscode-docker-context-extension"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
